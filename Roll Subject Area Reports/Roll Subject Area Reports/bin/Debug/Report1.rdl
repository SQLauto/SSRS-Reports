  <DataSets>
    <DataSet Name="qryData">
      <Query>
        <DataSourceName>EDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_RY">
            <Value>=Parameters!p_RY.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     AV.[Roll Year], AG.Area, AG.Jurisdiction, AG.Neighbourhood, AV.[Actual Value]
FROM        edw.FactActualValue AS AV INNER JOIN
                  edw.dimFolio AS FL ON AV.dimFolio_SK = FL.dimFolio_SK INNER JOIN
                  edw.dimAssessmentGeography AS AG ON FL.dimAssessmentGeography_SK = AG.dimAssessmentGeography_SK
WHERE     (AV.dimRollYear_SK = @p_RY) AND (AG.[Area Code] BETWEEN '01' AND '27')</CommandText>
      </Query>
      <Fields>
        <Field Name="Roll_Year">
          <DataField>Roll Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Area">
          <DataField>Area</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Jurisdiction">
          <DataField>Jurisdiction</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Neighbourhood">
          <DataField>Neighbourhood</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Actual_Value">
          <DataField>Actual Value</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="qryRY">
      <Query>
        <DataSourceName>EDW</DataSourceName>
        <CommandText>SELECT  
      RY.[dimRollYear_SK]
      ,RY.[dimRollYear_BK]
      ,RY.[RollYear]
      ,RY.[StartDate_BK]
      ,RY.[EndDate_BK]
  FROM [edw].[dimRollYear] RY
  WHERE EXISTS (SELECT 1 FROM [edw].[FactActualValue] AV WHERE RY.dimRollYear_SK = AV.dimRollYear_SK)</CommandText>
      </Query>
      <Fields>
        <Field Name="dimRollYear_SK">
          <DataField>dimRollYear_SK</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="dimRollYear_BK">
          <DataField>dimRollYear_BK</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RollYear">
          <DataField>RollYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="StartDate_BK">
          <DataField>StartDate_BK</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="EndDate_BK">
          <DataField>EndDate_BK</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="qryRCN">
