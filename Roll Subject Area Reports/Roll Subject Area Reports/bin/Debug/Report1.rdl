        <DataSourceName>EDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@p_RY">
            <Value>=Parameters!p_RY.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     dimRollCycle_SK, dimRollCycle_BK, [Roll Year], [Roll Entry Transaction Date], [Cycle Number], [Current Cycle Flag], [Roll Entry Run Date], [Final Roll Flag], [Preview Roll Flag]
FROM        edw.dimRollCycle RC
WHERE     RC.[Roll Year] = @p_RY</CommandText>
      </Query>
